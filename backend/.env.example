# Database
MONGODB_URI=mongodb://localhost:27017/timebase
JWT_SECRET=your_jwt_secret_key_here

# Free AI API Keys
# Hugging Face (Gratis) - https://huggingface.co/
HUGGINGFACE_API_TOKEN=hf_your_free_token_here

# Cohere AI (Free Tier) - https://cohere.ai/
COHERE_API_KEY=your_free_cohere_key_here

# Google Gemini (Free) - https://makersuite.google.com/
GEMINI_API_KEY=your_free_gemini_key_here

# OpenAI (Opsional, jika ada budget)
OPENAI_API_KEY=your_openai_key_here

# Server Config
NODE_ENV=development
PORT=3000
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Rate Limiting untuk Free APIs
AI_RATE_LIMIT_PER_HOUR=100
AI_FALLBACK_ENABLED=true
